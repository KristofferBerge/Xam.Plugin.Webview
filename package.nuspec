<?xml version="1.0"?>
<package>
  <metadata>
    <id>Xam.Plugin.WebView</id>
    <version>2.1.10</version>
    <title>WebView Plugin for Xamarin Forms</title>
    <authors>Ryan Dixon</authors>
    <owners>Ryan Dixon, Vistair</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A simple lightweight HybridWebView designed to work with Xamarin Forms</description>
    <releaseNotes>Expand the amount of url's which can be cancelled on Android</releaseNotes>
	<iconUrl>https://s18.postimg.org/wjfexgtnt/FWV.png</iconUrl>
    <tags>Plugins, Xamarin, WebView, Web, HTML, Forms, Android, iOS, UWP, WinRT</tags>
    <dependencies>
			<group targetFramework="portable-net45+win8+wpa81+wp8">
                <dependency id="Newtonsoft.Json" version="10.0.3"/>
			</group>
			<group targetFramework="netstandard1.0">
                <dependency id="Newtonsoft.Json" version="10.0.3"/>
				<dependency id="NETStandard.Library" version="1.6.1" />
			</group>
			<group targetFramework="uap">
                <dependency id="Newtonsoft.Json" version="10.0.3"/>
			</group>
            <group targetFramework="Xamarin.Mac20">
                <dependency id="Newtonsoft.Json" version="10.0.3"/>
            </group>
		</dependencies>
  </metadata>
  <files>
        <!-- Readme -->
        <file src="readme.txt" target="" />
        <!-- Core -->
		<file src="Xam.Plugin.WebView.Abstractions\bin\Release\netstandard1.0\Xam.Plugin.WebView.*" target="lib\netstandard1.0" />
		<!-- UWP -->
		<file src="Xam.Plugin.WebView.UWP\bin\Release\Xam.Plugin.WebView.*" target="lib\uap10.0" />
		<!-- Android -->
		<file src="Xam.Plugin.WebView.Droid\bin\Release\Xam.Plugin.WebView.*" target="lib\MonoAndroid10" />
		<!-- iOS -->
		<file src="Xam.Plugin.WebView.iOS\bin\Release\Xam.Plugin.WebView.*" target="lib\Xamarin.iOS10" />
        <!-- MacOS -->
		<file src="Xam.Plugin.WebView.MacOS\bin\Release\Xam.Plugin.WebView.*" target="lib\Xamarin.Mac20" />
  </files>
</package>